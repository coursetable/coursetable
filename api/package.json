{
  "scripts": {
    "prestart": "prisma db pull && prisma generate",
    "generate": "prisma generate",
    "start": "ts-node-dev --debug --files --exit-child src/server.ts",
    "prod": "ts-node --files src/server.ts",
    "checks": "bun run depcheck && bun run lint && bun run typecheck",
    "npm-checks": "npm run depcheck && npm run lint && npm run typecheck",
    "depcheck": "bunx depcheck . --json",
    "npm-depcheck": "npx depcheck . --json",
    "lint": "prettier -l . && eslint src",
    "typecheck": "tsc --noEmit && (tsc --noEmit --checkJs || true)"
  },
  "nodemonConfig": {
    "ignore": [
      "static/*"
    ]
  },
  "dependencies": {
    "@prisma/client": "5.4.2",
    "@sentry/node": "^7.73.0",
    "@sentry/tracing": "^7.73.0",
    "@types/app-root-path": "^1.2.6",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cookie-session": "^2.0.43",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/mysql": "^2.15.22",
    "@types/node": "^20.8.2",
    "@types/passport": "^1.0.13",
    "@types/psl": "^1.1.1",
    "app-root-path": "^3.1.0",
    "axios": "^1.5.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.5",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "0.5.0",
    "passport-cas": "git+https://github.com/coursetable/passport-cas",
    "prisma": "5.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0"
  }
}
