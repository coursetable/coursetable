    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/react": "^5.1.8",
    "@types/jquery": "^3.3.29",
    "@types/sequelize": "^4.27.36",
    "@types/storybook__react": "^4.0.2",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "^16.1.0",
