{
  "name": "coursetable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install-and-watch": "npm install --no-audit --loglevel verbose > nodeBuildLog.txt 2>&1 && npx webpack --watch >> nodeBuildLog.txt 2>&1",
    "prettier": "prettier **/*.{js,jsx,ts,tsx}",
    "build-crawler": "babel --out-dir dist/crawler --extensions .ts,.tsx,.js,.jsx --copy-files crawler",
    "watch-crawler": "babel --watch --out-dir dist/crawler --extensions .ts,.tsx,.js,.jsx --copy-files crawler",
    "storybook": "start-storybook -p 9001 -s client -c .storybook"
  },
  "browserslist": "defaults",
