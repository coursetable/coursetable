{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "tsc && vite build",
    "codegen": "cp ../api/static/seasons.json src/generated/seasons.json && graphql-codegen --config codegen.yml",
    "checks": "bun run depcheck && bun run lint",
    "depcheck": "bunx depcheck . --json",
    "lint": "bunx prettier -l . && bunx eslint src && node --loader=ts-node/esm scripts/resize-image.mts --test",
    "lint:fix": "bunx prettier --write . && bunx eslint --fix src && node --loader=ts-node/esm scripts/resize-image.mts",
    "serve": "vite preview",
    "start": "vite --https",
    "test": "vite test",
    "postinstall": "patch-package && cd node_modules/sharp && bun install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "graphql-language-service-interface": "2.4.2-alpha.2"
  },
  "dependencies": {
    "@apollo/client": "^3.8.5",
    "@graphiql/toolkit": "^0.9.1",
    "@sentry/react": "^7.73.0",
    "@types/react-helmet": "^6.1.9",
    "async-lock": "^1.4.0",
    "axios": "^1.5.1",
    "body-scroll-lock": "^3.1.5",
    "bootstrap": "^4.5.3",
    "chroma-js": "^2.4.2",
    "core-js": "^3.33.0",
    "file-saver": "^2.0.5",
    "gapi-script": "^1.2.0",
    "graphiql": "^3.0.6",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "ics": "^3.5.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "mark.js": "^8.11.1",
    "moment": "^2.29.4",
    "qs": "^6.11.2",
    "rc-slider": "9.7.1",
    "rc-tooltip": "^5.1.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^2.0.0",
    "react-big-calendar": "^0.30.0",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hotkeys": "^2.0.0",
    "react-icons": "^4.11.0",
    "react-lines-ellipsis": "^0.15.3",
    "react-multi-toggle": "^1.1.0",
    "react-router-dom": "^6.16.0",
    "react-scroll": "^1.8.9",
    "react-select": "^4.3.0",
    "react-toastify": "^9.1.3",
    "react-virtualized": "^9.22.5",
    "reactour": "^1.19.1",
    "styled-components": "^5.2.1",
    "ts-expect": "^1.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@types/async-lock": "^1.4.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/chroma-js": "^2.4.1",
    "@types/facebook-js-sdk": "^3.3.7",
    "@types/file-saver": "^2.0.5",
    "@types/gapi.auth2": "^0.0.60",
    "@types/gapi.calendar": "^3.0.9",
    "@types/lodash": "^4.14.199",
    "@types/luxon": "^3.3.2",
    "@types/mark.js": "^8.11.9",
    "@types/node": "^20.8.2",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.25",
    "@types/react-big-calendar": "^0.24.8",
    "@types/react-dom": "^18.2.10",
    "@types/react-scroll": "^1.8.8",
    "@types/react-select": "^3.0.26",
    "@types/react-virtualized": "^9.21.28",
    "@types/reactour": "^1.18.3",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
