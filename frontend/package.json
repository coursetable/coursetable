{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "tsc && vite build",
    "codegen": "cp ../api/static/seasons.json src/generated/seasons.json && graphql-codegen --config codegen.yml",
    "depcheck": "npx depcheck . --json",
    "lint-check": "prettier -l . && eslint src --quiet",
    "serve": "vite preview",
    "start": "vite --https",
    "test": "vite test",
    "postinstall": "patch-package"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
